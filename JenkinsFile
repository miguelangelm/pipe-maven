pipeline{
agent any
tools{
 maven 'maven 3.3.3'
 jdk 'jdk1.8.0_151'
}
stages{
  stage('Build'){
   steps{
    echo 'Building..'
    echo "PATH=${PATH}"
    echo "MAVEN=${M2_HOME}"
    sh 'mvn clean'
    }
  }
  stage('Test'){
   steps{
    echo 'Testing'
    }
  }
  stage('Deploy'){
   steps{
    echo 'Deploying'
    }
  }
}

post{
 always{
   archive "target/**/*"
   junit 'target/surefire-reports/*.xml'
 }
}



}